<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="BookWorm.Data"
                   namespace="BookWorm.Data.Models">


  <class name="Book">
    <id name="BookId">
      <generator class="native" />
    </id>
    <property name="Title" />
    <property name="Description" />
    <property name="CoverArt">
      <column name="CoverArt" sql-type="Varbinary(MAX)" length="2147483647"></column>
    </property>
    <property name="Rating" />
  </class>
  
</hibernate-mapping>
